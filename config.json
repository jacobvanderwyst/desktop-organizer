{
  "categories": {
    "Documents": {
      "extensions": [".pdf", ".doc", ".docx", ".txt", ".rtf", ".odt", ".pages"],
      "keywords": ["document", "paper", "report", "manual", "guide", "readme"]
    },
    "Images": {
      "extensions": [".jpg", ".jpeg", ".png", ".gif", ".bmp", ".tiff", ".svg", ".webp", ".ico"],
      "keywords": ["photo", "image", "picture", "screenshot", "wallpaper"]
    },
    "Videos": {
      "extensions": [".mp4", ".avi", ".mkv", ".mov", ".wmv", ".flv", ".webm", ".m4v"],
      "keywords": ["video", "movie", "clip", "film"]
    },
    "Audio": {
      "extensions": [".mp3", ".wav", ".flac", ".aac", ".ogg", ".m4a", ".wma"],
      "keywords": ["music", "audio", "sound", "podcast"]
    },
    "Archives": {
      "extensions": [".zip", ".rar", ".7z", ".tar", ".gz", ".bz2", ".xz"],
      "keywords": ["archive", "backup", "compressed", "package"]
    },
    "Applications": {
      "extensions": [".exe", ".msi", ".dmg", ".app", ".deb", ".rpm"],
      "keywords": ["installer", "setup", "application", "software"]
    },
    "Spreadsheets": {
      "extensions": [".xls", ".xlsx", ".csv", ".ods", ".numbers"],
      "keywords": ["spreadsheet", "data", "excel", "sheet"]
    },
    "Presentations": {
      "extensions": [".ppt", ".pptx", ".odp", ".key"],
      "keywords": ["presentation", "slides", "powerpoint", "keynote"]
    },
    "Code": {
      "extensions": [".py", ".js", ".html", ".css", ".java", ".cpp", ".c", ".php", ".rb", ".go", ".rs", ".ts"],
      "keywords": ["code", "script", "program", "source", "project"]
    },
    "Design": {
      "extensions": [".psd", ".ai", ".sketch", ".figma", ".xd"],
      "keywords": ["design", "mockup", "prototype", "ui", "graphics"]
    },
    "Ebooks": {
      "extensions": [".epub", ".mobi", ".azw", ".azw3"],
      "keywords": ["book", "ebook", "novel", "manual"]
    },
    "Fonts": {
      "extensions": [".ttf", ".otf", ".woff", ".woff2"],
      "keywords": ["font", "typeface"]
    }
  },
  "ignore_files": [
    ".DS_Store", 
    "Thumbs.db", 
    "desktop.ini",
    ".localized",
    "Icon\r"
  ],
  "dry_run": true,
  "create_backup": true,
  "ask_confirmation": true,
  "log_level": "INFO"
}